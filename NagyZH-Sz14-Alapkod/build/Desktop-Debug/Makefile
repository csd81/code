####### Compile

dragon.o: ../../dragon.cpp ../../dragon.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o dragon.o ../../dragon.cpp

knight.o: ../../knight.cpp ../../knight.h \
		../../warrior.h \
		../../dragon.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o knight.o ../../knight.cpp

mage.o: ../../mage.cpp ../../mage.h \
		../../warrior.h \
		../../dragon.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o mage.o ../../mage.cpp

main.o: ../../main.cpp ../../tavern.h \
		../../warrior.h \
		../../dragon.h \
		../../json.hpp \
		../../party.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o ../../main.cpp

party.o: ../../party.cpp ../../party.h \
		../../warrior.h \
		../../dragon.h \
		../../tavern.h \
		../../json.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o party.o ../../party.cpp

ranger.o: ../../ranger.cpp ../../ranger.h \
		../../warrior.h \
		../../dragon.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o ranger.o ../../ranger.cpp

tavern.o: ../../tavern.cpp ../../tavern.h \
		../../warrior.h \
		../../dragon.h \
		../../json.hpp \
		../../knight.h \
		../../mage.h \
		../../ranger.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o tavern.o ../../tavern.cpp

warrior.o: ../../warrior.cpp ../../warrior.h \
		../../dragon.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o warrior.o ../../warrior.cpp

